@article{Lister:2013et,
author = {Lister, Ryan and Mukamel, Eran A and Nery, Joseph R and Urich, Mark and Puddifoot, Clare A and Johnson, Nicholas D and Lucero, Jacinta and Huang, Yun and Dwork, Andrew J and Schultz, Matthew D and Yu, Miao and Tonti-Filippini, Julian and Heyn, Holger and Hu, Shijun and Wu, Joseph C and Rao, Anjana and Esteller, Manel and He, Chuan and Haghighi, Fatemeh G and Sejnowski, Terrence J and Behrens, M Margarita and Ecker, Joseph R},
title = {{Global Epigenomic Reconfiguration During Mammalian Brain Development.}},
journal = {Science},
year = {2013},
month = jul,
affiliation = {Genomic Analysis Laboratory, The Salk Institute for Biological Studies, La Jolla, CA 92037, USA.},
doi = {10.1126/science.1237905},
pmid = {23828890},
language = {English},
read = {Yes},
rating = {0},
date-added = {2013-07-30T01:51:03GMT},
date-modified = {2014-03-16T21:22:39GMT},
abstract = {DNA methylation is implicated in mammalian brain development and plasticity underlying learning and memory. We report the genome-wide composition, patterning, cell specificity, and dynamics of DNA methylation at single-base resolution in human and mouse frontal cortex throughout their lifespan. Widespread methylome reconfiguration occurs during fetal to young adult development, coincident with synaptogenesis. During this period, highly conserved non-CG methylation (mCH) accumulates in neurons, but not glia, to become the dominant form of methylation in the human neuronal genome. Moreover, we found an mCH signature that identifies genes escaping X-chromosome inactivation. Finally, whole-genome single-base resolution 5-hydroxymethylcytosine (hmC) maps revealed that hmC marks fetal brain cell genomes at putative regulatory regions that are CG-demethylated and activated in the adult brain, and that CG demethylation at these hmC-poised loci depends on Tet2 activity.},
